<script>
$.ajax({
	url: 'https://www.hankyung.com/ext-api/promotion'
	, success: function(data){
		$.each(data, function(idx, val){
			$(".promotion-nav-wrap .promotion-nav").append('\
				<div class="item">\
					<a href="'+val.link+'" '+(val.isblank == 'Y' ? 'target="_blank"' : '')+'>'+val.title+'</a>\
				</div>\
			');
			
			$(".promotion-nav-wrap .promotion-nav-all ul").append('<li><a href="'+val.link+'" '+(val.isblank == 'Y' ? 'target="_blank"' : '')+'>'+val.title+'</a></li>');
		});
		
		if(data.length > 1){
			setGNBPromotionSlider();
		}
	}
});

function setGNBPromotionSlider(){
	if($.fn.slick == undefined){
		$('head').append('<link rel="stylesheet" href="//static.hankyung.com/plugin/slick/slick.custom.css">');
		
		slickScript = document.createElement('script');
		slickScript.src = "//static.hankyung.com/plugin/slick/slick.min.js";
		slickScript.onload = function(){
			$('.promotion-nav').slick({
				vertical: true,
				verticalSwiping: true,
				arrows: false,
				dots: false,
				autoplay: true,
				speed: 300
			});
		}
		
		document.head.appendChild(slickScript);
	}else{
		$('.promotion-nav').slick({
			vertical: true,
			verticalSwiping: true,
			arrows: false,
			dots: false,
			autoplay: true,
			speed: 300
		});
	}

	$('.promotion-nav a, .promotion-nav-all').on({
		mouseenter: function () {
			$('.promotion-nav-all').addClass('on');
		}
		, mouseleave: function () {
			$('.promotion-nav-all').removeClass('on');
		}
	});
}
</script>

